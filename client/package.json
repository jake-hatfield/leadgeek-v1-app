{
	"engines": {
		"node": "14.15.3"
	},
	"name": "leadgeek-client",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@craco/craco": "^6.4.0",
		"@reduxjs/toolkit": "^1.6.2",
		"@stripe/react-stripe-js": "^1.6.0",
		"@stripe/stripe-js": "^1.21.1",
		"@tailwindcss/forms": "^0.3.4",
		"@types/luxon": "^2.0.7",
		"@types/react": "^17.0.34",
		"@types/react-csv": "^1.1.2",
		"@types/react-dom": "^17.0.11",
		"@types/react-gtm-module": "^2.0.1",
		"@types/react-image-magnify": "^2.7.1",
		"@types/react-router-dom": "^5.3.2",
		"autoprefixer": "^9.8.8",
		"axios": "^0.20.0",
		"craco-alias": "^3.0.1",
		"luxon": "^1.28.0",
		"luxon-business-days": "^2.8.2",
		"react": "^16.14.0",
		"react-content-loader": "^6.0.3",
		"react-csv": "^2.0.3",
		"react-dom": "^16.14.0",
		"react-gtm-module": "^2.0.11",
		"react-hotkeys-hook": "^3.4.4",
		"react-image-magnify": "^2.7.4",
		"react-preloader-icon": "^0.4.1",
		"react-redux": "^7.2.6",
		"react-router": "^5.2.1",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"react-spring": "^9.3.0",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
		"typescript": "^4.4.4",
		"use-state-if-mounted": "^1.0.4"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"test": "craco test",
		"test:coverage": "craco test --coverage --runInBand --watchAll=false",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
		"eject": "react-scripts eject",
		"format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
		"isready": "npm run format && npm run lint && npm run test:coverage && npm run build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"proxy": "http://localhost:5000/",
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts",
			"!src/index.tsx",
			"!src/serviceWorker.ts",
			"!src/reportWebVitals.ts"
		],
		"coveragePathIgnorePatterns": [
			"./src/*/*.types.{ts,tsx}",
			"./src/index.tsx",
			"./src/serviceWorker.ts"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"text-summary",
			"clover"
		],
		"coverageThreshold": {
			"global": {
				"statements": 95,
				"branches": 95,
				"lines": 95,
				"functions": 95
			}
		},
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
			"src/(.*)$": "<rootDir>/src/$1"
		}
	},
	"devDependencies": {
		"@types/enzyme": "3.10.10",
		"@types/enzyme-adapter-react-16": "1.0.6",
		"@types/jest": "^27.0.2",
		"babel-eslint": "^10.1.0",
		"babel-plugin-react-remove-properties": "^0.3.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.6",
		"enzyme-to-json": "3.6.2",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.10.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"jest-fetch-mock": "3.0.3",
		"ts-jest": "27.0.7"
	}
}
